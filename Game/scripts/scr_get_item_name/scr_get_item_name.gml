///@desc scr_get_item_name(item_index)
///@arg0 item_index

var item_ = argument0;

if item_ == item_speed_boost_ {
	return "Speed Boost"	
}
if item_ == item_turne_around_ {
	return "Turne Around"	
}
if item_ == item_shield_ {
	return "Shield"	
}
if item_ == item_invisibility_ {
	return "Invisibility"	
}